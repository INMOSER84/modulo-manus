<odoo>
    <data noupdate="1">
        <!-- Tipos de equipo -->
        <record id="equipment_type_laptop" model="service.equipment.type">
            <field name="name">Laptop</field>
            <field name="code">LAP</field>
            <field name="description">Equipos portátiles</field>
            <field name="active">True</field>
        </record>
        
        <record id="equipment_type_desktop" model="service.equipment.type">
            <field name="name">Desktop</field>
            <field name="code">DES</field>
            <field name="description">Equipos de escritorio</field>
            <field name="active">True</field>
        </record>
        
        <record id="equipment_type_printer" model="service.equipment.type">
            <field name="name">Impresora</field>
            <field name="code">PRI</field>
            <field name="description">Equipos de impresión</field>
            <field name="active">True</field>
        </record>
        
        <record id="equipment_type_server" model="service.equipment.type">
            <field name="name">Servidor</field>
            <field name="code">SER</field>
            <field name="description">Equipos servidores</field>
            <field name="active">True</field>
        </record>
        
        <record id="equipment_type_network" model="service.equipment.type">
            <field name="name">Red</field>
            <field name="code">NET</field>
            <field name="description">Equipos de red</field>
            <field name="active">True</field>
        </record>
        
        <!-- Marcas -->
        <record id="brand_dell" model="service.equipment.brand">
            <field name="name">Dell</field>
            <field name="code">DEL</field>
            <field name="description">Marca Dell</field>
            <field name="active">True</field>
        </record>
        
        <record id="brand_hp" model="service.equipment.brand">
            <field name="name">HP</field>
            <field name="code">HP</field>
            <field name="description">Marca HP</field>
            <field name="active">True</field>
        </record>
        
        <record id="brand_lenovo" model="service.equipment.brand">
            <field name="name">Lenovo</field>
            <field name="code">LEN</field>
            <field name="description">Marca Lenovo</field>
            <field name="active">True</field>
        </record>
        
        <record id="brand_asus" model="service.equipment.brand">
            <field name="name">ASUS</field>
            <field name="code">ASU</field>
            <field name="description">Marca ASUS</field>
            <field name="active">True</field>
        </record>
        
        <record id="brand_canon" model="service.equipment.brand">
            <field name="name">Canon</field>
            <field name="code">CAN</field>
            <field name="description">Marca Canon</field>
            <field name="active">True</field>
        </record>
        
        <!-- Modelos Dell -->
        <record id="model_inspiron" model="service.equipment.model">
            <field name="name">Inspiron</field>
            <field name="code">INS</field>
            <field name="brand_id" ref="brand_dell"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo Inspiron de Dell</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_latitude" model="service.equipment.model">
            <field name="name">Latitude</field>
            <field name="code">LAT</field>
            <field name="brand_id" ref="brand_dell"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo Latitude de Dell</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_optiplex" model="service.equipment.model">
            <field name="name">OptiPlex</field>
            <field name="code">OPT</field>
            <field name="brand_id" ref="brand_dell"/>
            <field name="equipment_type_id" ref="equipment_type_desktop"/>
            <field name="description">Modelo OptiPlex de Dell</field>
            <field name="active">True</field>
        </record>
        
        <!-- Modelos HP -->
        <record id="model_pavilion" model="service.equipment.model">
            <field name="name">Pavilion</field>
            <field name="code">PAV</field>
            <field name="brand_id" ref="brand_hp"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo Pavilion de HP</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_elitebook" model="service.equipment.model">
            <field name="name">EliteBook</field>
            <field name="code">ELI</field>
            <field name="brand_id" ref="brand_hp"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo EliteBook de HP</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_laserjet" model="service.equipment.model">
            <field name="name">LaserJet</field>
            <field name="code">LAS</field>
            <field name="brand_id" ref="brand_hp"/>
            <field name="equipment_type_id" ref="equipment_type_printer"/>
            <field name="description">Modelo LaserJet de HP</field>
            <field name="active">True</field>
        </record>
        
        <!-- Modelos Lenovo -->
        <record id="model_thinkpad" model="service.equipment.model">
            <field name="name">ThinkPad</field>
            <field name="code">THK</field>
            <field name="brand_id" ref="brand_lenovo"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo ThinkPad de Lenovo</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_ideapad" model="service.equipment.model">
            <field name="name">IdeaPad</field>
            <field name="code">IDE</field>
            <field name="brand_id" ref="brand_lenovo"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo IdeaPad de Lenovo</field>
            <field name="active">True</field>
        </record>
        
        <!-- Modelos ASUS -->
        <record id="model_vivobook" model="service.equipment.model">
            <field name="name">VivoBook</field>
            <field name="code">VIV</field>
            <field name="brand_id" ref="brand_asus"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo VivoBook de ASUS</field>
            <field name="active">True</field>
        </record>
        
        <record id="model_rog" model="service.equipment.model">
            <field name="name">ROG</field>
            <field name="code">ROG</field>
            <field name="brand_id" ref="brand_asus"/>
            <field name="equipment_type_id" ref="equipment_type_laptop"/>
            <field name="description">Modelo ROG de ASUS</field>
            <field name="active">True</field>
        </record>
        
        <!-- Modelos Canon -->
        <record id="model_pixma" model="service.equipment.model">
            <field name="name">PIXMA</field>
            <field name="code">PIX</field>
            <field name="brand_id" ref="brand_canon"/>
            <field name="equipment_type_id" ref="equipment_type_printer"/>
            <field name="description">Modelo PIXMA de Canon</field>
            <field name="active">True</field>
        </record>
        
        <!-- Tipos de servicio -->
        <record id="service_type_maintenance" model="service.type">
            <field name="name">Mantenimiento Preventivo</field>
            <field name="code">MP</field>
            <field name="description">Mantenimiento preventivo de equipos</field>
            <field name="price">150.00</field>
            <field name="duration">2.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_repair" model="service.type">
            <field name="name">Reparación General</field>
            <field name="code">RG</field>
            <field name="description">Reparación general de equipos</field>
            <field name="price">200.00</field>
            <field name="duration">3.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_installation" model="service.type">
            <field name="name">Instalación</field>
            <field name="code">INS</field>
            <field name="description">Instalación de equipos y software</field>
            <field name="price">100.00</field>
            <field name="duration">1.50</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_diagnosis" model="service.type">
            <field name="name">Diagnóstico</field>
            <field name="code">DIA</field>
            <field name="description">Diagnóstico de fallas</field>
            <field name="price">80.00</field>
            <field name="duration">1.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_cleaning" model="service.type">
            <field name="name">Limpieza</field>
            <field name="code">LIM</field>
            <field name="description">Limpieza interna y externa</field>
            <field name="price">60.00</field>
            <field name="duration">1.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_format" model="service.type">
            <field name="name">Formateo</field>
            <field name="code">FOR</field>
            <field name="description">Formateo e instalación de SO</field>
            <field name="price">120.00</field>
            <field name="duration">2.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_upgrade" model="service.type">
            <field name="name">Actualización</field>
            <field name="code">ACT</field>
            <field name="description">Actualización de hardware/software</field>
            <field name="price">180.00</field>
            <field name="duration">2.50</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_recovery" model="service.type">
            <field name="name">Recuperación de Datos</field>
            <field name="code">REC</field>
            <field name="description">Recuperación de datos perdidos</field>
            <field name="price">250.00</field>
            <field name="duration">4.00</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_network" model="service.type">
            <field name="name">Configuración de Red</field>
            <field name="code">RED</field>
            <field name="description">Configuración de redes</field>
            <field name="price">130.00</field>
            <field name="duration">1.50</field>
            <field name="active">True</field>
        </record>
        
        <record id="service_type_virus" model="service.type">
            <field name="name">Eliminación de Virus</field>
            <field name="code">VIR</field>
            <field name="description">Eliminación de virus y malware</field>
            <field name="price">90.00</field>
            <field name="duration">1.50</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
