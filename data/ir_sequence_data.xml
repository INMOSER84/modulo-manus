<odoo>
    <data noupdate="1">
        <!-- Secuencia para órdenes de servicio -->
        <record id="sequence_service_order" model="ir.sequence">
            <field name="name">Secuencia de Órdenes de Servicio</field>
            <field name="code">service.order</field>
            <field name="prefix">SO-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>
        
        <!-- Secuencia para equipos de servicio -->
        <record id="sequence_service_equipment" model="ir.sequence">
            <field name="name">Secuencia de Equipos de Servicio</field>
            <field name="code">service.equipment</field>
            <field name="prefix">EQ-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>
        
        <!-- Secuencia para tipos de servicio -->
        <record id="sequence_service_type" model="ir.sequence">
            <field name="name">Secuencia de Tipos de Servicio</field>
            <field name="code">service.type</field>
            <field name="prefix">ST-</field>
            <field name="padding">3</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>
        
        <!-- Secuencia para historial de stock -->
        <record id="sequence_stock_history" model="ir.sequence">
            <field name="name">Secuencia de Historial de Stock</field>
            <field name="code">stock.history</field>
            <field name="prefix">SH-</field>
            <field name="padding">5</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
            <field name="implementation">standard</field>
            <field name="active">True</field>
        </record>
    </data>
</odoo>
