<odoo>
    <record id="fsm_order_view_form_inmoser_extension" model="ir.ui.view">
        <field name="name">fsm.order.form.inmoser.extension</field>
        <field name="model">fsm.order</field>
        <field name="inherit_id" ref="industry_fsm.fsm_order_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Detalles de Servicio Inmoser" name="inmoser_service_details">
                    <group>
                        <field name="x_inmoser_service_order_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
