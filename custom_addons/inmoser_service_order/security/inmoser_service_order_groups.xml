<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="group_inmoser_service_order_user" model="res.groups">
            <field name="name">Inmoser Service Order / User</field>
            <field name="category_id" ref="base.module_category_services"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>

        <record id="group_inmoser_service_order_manager" model="res.groups">
            <field name="name">Inmoser Service Order / Manager</field>
            <field name="category_id" ref="base.module_category_services"/>
            <field name="implied_ids" eval="[(4, ref('group_inmoser_service_order_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        </record>

        <record id="group_inmoser_technician" model="res.groups">
            <field name="name">Inmoser / Technician</field>
            <field name="category_id" ref="base.module_category_services"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
    </data>
</odoo>
