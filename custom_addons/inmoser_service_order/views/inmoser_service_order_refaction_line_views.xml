<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_inmoser_service_order_refaction_line_tree" model="ir.ui.view">
        <field name="name">inmoser.service.order.refaction.line.tree</field>
        <field name="model">inmoser.service.order.refaction.line</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="product_id"/>
                <field name="quantity"/>
                <field name="price_unit"/>
            </tree>
        </field>
    </record>
    <record id="view_inmoser_service_order_refaction_line_form" model="ir.ui.view">
        <field name="name">inmoser.service.order.refaction.line.form</field>
        <field name="model">inmoser.service.order.refaction.line</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="product_id"/>
                        <field name="quantity"/>
                        <field name="price_unit"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
